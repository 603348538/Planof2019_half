module voltmeter(
	key1,
	key2,
	rst,
	clk,
	lock595,
	out595,
	clk595,
	adc_sclk,
	adc_saddr,
	adc_csn,
	adc_sdat
);
input clk,rst;
wire [2:0]out;
output lock595,out595ï¼Œclk595;
wire [31:0]data;

wire BUSY;
wire START;

KEY_Selector KEY_Selector1(
	.key1(key1),
	.key2(key2),
	.clk(clk),
	.rst(rst),
	.out(out)
);


BCDDisplay BCDDisplay1(
	.clk(clk),
	.rst(rst),
	.data(data),
	.lock595(lock595),
	.out595(out595),
	.clk595(clk595)
);

ADC128S022_interface ADC128S022_interface1(
	.ADDR(out),
	.DATA(data[11:0]),
	.START(START),
	.BUSY(BUSY),
	.CLK(clk),
	.RST(rst),
	.adc_sclk(adc_sclk),
	.adc_saddr(adc_saddr),
	.adc_csn(adc_csn),
	.adc_sdat(adc_sdat)
);

//always@(posedge clk)
assign START = 1;

endmodule